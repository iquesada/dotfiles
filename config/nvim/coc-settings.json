
{
  "coc.preferences.colorSupport": false,
  "coc.preferences.extensionUpdateCheck": "weekly",
  "coc.preferences.formatOnSaveFiletypes": [
    "elixir",
    "json",
    "js"
  ],
  "coc.preferences.useQuickfixForLocations": true,
  "codeLens.enable": true,
  "diagnostic.messageTarget": "float",
  "diagnostic.enableMessage": "always",
  "diagnostic.maxWindowHeight": 20,
  "diagnostic.checkCurrentLine": true,
  "diagnostic.refreshAfterSave": false,
  "eslint.autoFixOnSave": true,
  "emmet.includeLanguages": {
    "*.erb": "html",
    "ruby": "html"
  },
  "javascript.format.enabled": true,
  "suggest.enablePreview": true,
  "suggest.languageSourcePriority": 100,
  "suggest.localityBonus": false,
  "suggest.minTriggerInputLength": 1,
  "suggest.noselect": false,
  "suggest.removeDuplicateItems": true,
  "tsserver.enableJavascript": true,
  "tsserver.formatOnType": false,
  "yank.enableCompletion": false,
  "yank.highlight.enable": false,
  "yank.priority": 40,
  "languageserver": {
    "elm": {
      "trace.server": "verbose",
      "command": "elm-language-server",
      "filetypes": [
        "elm"
      ],
      "rootPatterns": [
        "elm.json"
      ],
      "initializationOptions": {
        "elmAnalyseTrigger": "save"
      }
    }
  }
}
